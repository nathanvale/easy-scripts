  "license": "MIT",
  "author": "Nathan Vale <hi@nathanvale.com>",
  "homepage": "https://github.com/nathanvale/easy-scripts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanvale/easy-scripts.git"
  },
  "bugs": {
    "url": "https://github.com/nathanvale/easy-scripts/issues"
  },
  "version": "0.0.0-semantically-released",
  "bin": {
    "easy-scripts": "dist/index.js"
  },
  "files": [
    "config",
    "config.js",
    "dist"
  ],
  "scripts": {
    "add-contributor": "node src contributors add",
    "build": "node src build",
    "commit": "commit",
    "format": "node src format",
    "lint": "node src lint",
    "semantic-release": "semantic-release",
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "validate": "node src validate"
