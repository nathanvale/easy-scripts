{
  "name": "ndv-scripts",
  "version": "0.0.0-semantically-released",
  "description": "This is a CLI that abstracts away the configuration of my open source projects. Why copy and paste the configuration of jest, prettier, commitlint, babel builds and more when you can share all this on npm?",
  "homepage": "https://github.com/nathanvale/ndv-scripts#readme",
  "bugs": {
    "url": "https://github.com/nathanvale/ndv-scripts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nathanvale/ndv-scripts.git"
  },
  "license": "MIT",
  "author": "Nathan Vale <hi@nathanvale.com>",
  "files": [
    "dist",
    "config",
    "config.js"
  ],
  "bin": {
    "ndv-scripts": "dist/index.js"
  },
  "scripts": {
    "add-contributor": "node src contributors add",
    "build": "node src build",
    "commit": "commit",
    "format": "node src format",
    "lint": "node src lint",
    "semantic-release": "semantic-release",
    "test": "node src test",
    "test:update": "node src test --updateSnapshot",
    "validate": "node src validate"
  },
